CompileFlags:
    Add: [
        "-xc",
        "-std=c99",
        "-I./include/",
        "-lm",
        "-Wall",
        "-Wextra",
        "-Wpedantic",
        "-Wunused",
        "-Wfloat-equal",
        "-Wundef",
        "-Wshadow",
        "-Wpointer-arith",
        "-Wcast-align",
        "-Wstrict-prototypes",
        "-Wstrict-overflow=5",
        "-Wwrite-strings",
        "-Wcast-qual",
        "-Wswitch-default",
        "-Wswitch-enum",
        "-Wconversion",
        "-Wvla",
        "-Wunreachable-code"
        ]
    Compiler: gcc
